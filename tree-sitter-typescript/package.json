{
  "name": "tree-sitter-typescript-mcpsaver",
  "version": "0.23.3",
  "description": "TypeScript and TSX grammars for tree-sitter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FosterG4/tree-sitter-mcpsaver.git"
  },
  "license": "MIT",
  "author": "Max Brunsfeld <maxbrunsfeld@gmail.com>",
  "maintainers": [
    "Amaan Qureshi <amaanq12@gmail.com>"
  ],
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "typescript",
    "tsx"
  ],
  "files": [
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "typescript/grammar.js",
    "tsx/grammar.js",
    "tree-sitter.json",
    "typescript/package.json",
    "tsx/package.json",
    "typescript/src/**",
    "tsx/src/**",
    "common/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4",
    "tree-sitter-javascript": "file:../tree-sitter-javascript"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "eslint": ">=9.34.0",
    "eslint-config-treesitter": "^1.0.2",
    "tree-sitter-cli": "^0.25.8",
    "prebuildify": "^6.0.1"
  },
  "scripts": {
    "install": "node-gyp-build",
    "lint": "eslint common/define-grammar.js",
    "test": "node --test bindings/node/*_test.js"
  }
}
