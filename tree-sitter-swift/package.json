{
  "name": "tree-sitter-swift-mcpsaver",
  "version": "0.7.2",
  "description": "A tree-sitter grammar for the Swift programming language.",
  "main": "bindings/node/index.js",
  "types": "bindings/node",
  "scripts": {
    "install": "node-gyp-build",
    "postinstall": "node-gyp configure && node-gyp build",
    "build": "tree-sitter generate --no-bindings",
    "build-wasm": "tree-sitter build --wasm",
    "ci": "prettier --check grammar.js",
    "ci-fix": "prettier --write grammar.js",
    "test-ci": "./scripts/test-with-memcheck.sh --install-valgrind",
    "test": "./scripts/test-with-memcheck.sh",
    "prebuildify": "prebuildify --napi --strip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FosterG4/tree-sitter-mcpsaver.git"
  },
  "tree-sitter": [
    {
      "scope": "source.swift",
      "file-types": [
        "swift"
      ],
      "injection-regex": "swift",
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm",
      "injections": "queries/injections.scm"
    }
  ],
  "keywords": [
    "parser",
    "swift"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "scripts/*",
    "src/**"
  ],
  "author": {
    "name": "mcpsaver",
    "email": "dikkyhardian@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alex-pinkus/tree-sitter-swift/issues"
  },
  "homepage": "https://github.com/alex-pinkus/tree-sitter-swift#readme",
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4",
    "tree-sitter-cli": "^0.25",
    "which": "5.0.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "node-gyp": "^11.4.1",
    "prebuildify": "^6.0.1",
    "prettier": "3.6.2"
  }
}
