{
  "name": "tree-sitter-julia-mcpsaver",
  "version": "0.23.2",
  "description": "Julia grammar for tree-sitter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FosterG4/tree-sitter-mcpsaver.git"
  },
  "license": "MIT",
  "author": {
    "name": "Max Brunsfeld",
    "email": "maxbrunsfeld@gmail.com"
  },
  "main": "bindings/node",
