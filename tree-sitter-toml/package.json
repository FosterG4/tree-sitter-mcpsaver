{
  "name": "tree-sitter-toml-mcpsaver",
  "version": "0.5.2",
  "description": "TOML grammar for tree-sitter",
  "keywords": [
    "parser",
    "lexer"
  ],
  "main": "bindings/node",
  "types": "bindings/node",
  "repository": "https://github.com/FosterG4/tree-sitter-mcpsaver",
  "homepage": "https://github.com/ikatyang/tree-sitter-toml#readme",
  "author": {
    "name": "Ika",
    "email": "ikatyang@gmail.com",
    "url": "https://github.com/ikatyang"
  },
  "license": "MIT",
  "scripts": {
    "install": "node-gyp-build",
    "lint": "eslint grammar.js",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "standard-version": {
    "preset": "angular",
    "scripts": {
      "postbump": "rm -r docs && node scripts/generate-playground.js && git add docs"
    }
  },
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": ">=9.34.0",
    "eslint-config-treesitter": "^1.0.2",
    "prebuildify": "^6.0.1",
    "regexp-util": "2.0.3",
    "standard-version": "9.5.0",
    "tree-sitter-cli": "^0.25.8"
  },
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "tree-sitter": [
    {
      "scope": "source.toml",
      "file-types": [
        "toml"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "^toml$"
    }
  ]
}
