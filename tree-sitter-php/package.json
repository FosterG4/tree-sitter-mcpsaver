{
  "name": "tree-sitter-php-mcpsaver",
  "version": "0.24.3",
  "description": "PHP grammar for tree-sitter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FosterG4/tree-sitter-mcpsaver.git"
  },
  "license": "MIT",
  "author": {
    "name": "mcpsaver",
    "email": "dikkyhardian@gmail.com"
  },
  
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "php"
  ],
  "files": [
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "php/grammar.js",
    "php_only/grammar.js",
    "tree-sitter.json",
    "php/package.json",
    "php_only/package.json",
    "php/src/**",
    "php_only/src/**",
    "common/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "eslint": "^9.34.0",
    "eslint-config-treesitter": "^1.0.2",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.8"
  },
  "scripts": {
    "install": "node-gyp-build",
    "lint": "eslint common/define-grammar.js",
    "test": "node --test bindings/node/*_test.js"
  }
}
